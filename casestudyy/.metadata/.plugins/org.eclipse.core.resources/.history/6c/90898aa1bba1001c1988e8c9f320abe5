package com.te.string;

public class ConvertStringtoint {

	public static void main(String[] args) {
		// string to int
       String s="20";
       int i = Integer.parseInt(s);
       System.out.println(i);
       //int to string
       int i1=20;
       String valueOf = String.valueOf(i1);
       System.out.println(valueOf);
	}

}
