package Com.te.logicprogram;

public class Largestof3 {

	public static void main(String[] args) {
		double d1=88.9,d2=34.3,d3=33.44;
		if(d1>=d2 && d1>=d3) {
			System.out.println("largest 1");
		}else if(d2>=d1 && d2>=d3) {
			System.out.println("largest 2");
		}else {
			System.out.println("no largest");
		}
		

	}

}
